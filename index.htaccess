# Set an efficient cache policy for common.css
<IfModule mod_expires.c>
   ExpiresActive on
   ExpiresByType text/css "access plus 1 year"
</IfModule>
# Set an efficient cache policy for media.css
<IfModule mod_expires.c>
   ExpiresActive on
   ExpiresByType text/css "access plus 1 year"
</IfModule>
# Set an efficient cache policy for index.js
<IfModule mod_expires.c>
   ExpiresActive on
   ExpiresByType text/javascript "access plus 1 year"
</IfModule>
